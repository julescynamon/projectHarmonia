{
  "name": "naturopath-website",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "notify": "NODE_OPTIONS='--loader ts-node/esm' node --experimental-specifier-resolution=node src/scripts/send-notifications.ts",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:unit": "vitest run --include='src/**/*.test.ts'",
    "test:integration": "npm run build && npm run preview & sleep 5 && node tests/integration/run-tests.js",
    "test:integration:watch": "vitest run --include='src/tests/integration/**/*.test.ts' --watch",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:dev": "npm run dev & sleep 15 && BASE_URL=http://localhost:4322 playwright test --project=chromium",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:local": "NODE_ENV=test playwright test --project=chromium",
    "test:all": "npm run test:run && npm run test:integration && npm run test:e2e",
    "test:ci": "npm run test:run && npm run test:e2e",
    "test:setup": "node scripts/setup-test-env.js",
    "test:validate": "tsx scripts/validate-test-env.ts",
    "playwright:install": "playwright install",
    "playwright:codegen": "playwright codegen http://localhost:4322",
    "test:e2e:report": "playwright show-report",
    "test:logging": "tsx src/scripts/test-logging.ts",
    "test:monitoring": "tsx src/scripts/test-monitoring.ts",
    "test:csp": "tsx src/scripts/test-csp.ts",
    "test:cors": "tsx scripts/test-cors.ts",
    "test:security": "tsx scripts/test-security.ts",
    "test:rate-limiting": "tsx src/scripts/test-rate-limiting.ts",
    "test:rate-limit-http": "node scripts/test-rate-limit-http.js",
    "test:performance": "node scripts/test-performance.js",
    "test:email-templates": "tsx src/scripts/test-email-templates.ts",
    "test:availability": "tsx src/scripts/test-availability.ts",
    "test:admin-disponibilites": "tsx src/scripts/test-admin-disponibilites.ts",
    "test:admin-appointments": "tsx src/scripts/test-admin-appointments.ts",
    "fix-admin-profile": "tsx src/scripts/fix-admin-profile.ts",
    "test-admin-access": "tsx src/scripts/test-admin-access.ts",
    "test-admin-pages": "tsx src/scripts/test-admin-pages.ts",
    "test-admin-authorization": "tsx src/scripts/test-admin-authorization.ts",
    "test-admin-disponibilites-tabs": "tsx src/scripts/test-admin-disponibilites-tabs.ts",
    "test-admin-disponibilites-functionality": "tsx src/scripts/test-admin-disponibilites-functionality.ts",
    "test-admin-disponibilites-guide": "tsx src/scripts/test-admin-disponibilites-guide.ts",
    "test-admin-disponibilites-supabase": "tsx src/scripts/test-admin-disponibilites-supabase.ts",
    "test-admin-disponibilites-supabase-fix": "tsx src/scripts/test-admin-disponibilites-supabase-fix.ts",
    "test:performance:build": "npm run build && npm run preview & sleep 5 && npm run test:performance",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:debug": "playwright test --debug",
    "lint": "eslint . --ext .js,.ts,.astro",
    "lint:fix": "eslint . --ext .js,.ts,.astro --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "astro check",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:status": "supabase status",
    "supabase:reset": "supabase db reset",
    "supabase:diff": "supabase db diff",
    "migration:create": "node scripts/migration-workflow.js create",
    "migration:list": "node scripts/migration-workflow.js list",
    "migration:apply": "node scripts/migration-workflow.js apply",
    "migration:validate": "node scripts/migration-workflow.js validate",
    "migration:backup": "node scripts/migration-workflow.js backup",
    "migration:report": "node scripts/migration-workflow.js report",
    "migration:interactive": "node scripts/migration-workflow.js interactive",
    "migration:helper": "./scripts/migration-helper.sh",
    "validate:secrets": "node scripts/validate-github-secrets.js",
    "ci:validate": "npm run validate:secrets && npm run lint && npm run test:run"
  },
  "dependencies": {
    "@astrojs/node": "^9.4.3",
    "@astrojs/react": "^4.3.0",
    "@astrojs/sitemap": "^3.5.1",
    "@astrojs/tailwind": "^6.0.2",
    "@emailjs/browser": "^4.4.1",
    "@fontsource/cormorant-garamond": "^5.0.8",
    "@fontsource/great-vibes": "^5.0.8",
    "@fontsource/lato": "^5.0.8",
    "@fontsource/lora": "^5.1.1",
    "@fontsource/playfair-display": "^5.1.1",
    "@fontsource/raleway": "^5.1.1",
    "@highlight-run/node": "^3.12.15",
    "@stripe/stripe-js": "^2.3.0",
    "@supabase/supabase-js": "^2.48.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/typography": "^0.5.10",
    "@types/marked": "^6.0.0",
    "@types/nodemailer": "^6.4.17",
    "@types/pino": "^7.0.5",
    "@types/sanitize-html": "^2.13.0",
    "aos": "^2.3.4",
    "astro": "^5.13.3",
    "astro-compress": "^2.2.15",
    "dotenv": "^16.4.7",
    "gray-matter": "^4.0.3",
    "gsap": "^3.12.4",
    "marked": "^15.0.7",
    "nanostores": "^0.11.3",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.16",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "resend": "^4.6.0",
    "sanitize-html": "^2.14.0",
    "solid-js": "^1.9.4",
    "sonner": "^1.7.4",
    "stripe": "^14.12.0",
    "tailwindcss": "^3.4.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.53.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20.17.16",
    "@vitest/ui": "^3.2.4",
    "get-port": "^7.1.0",
    "jsdom": "^26.1.0",
    "sharp": "^0.34.2",
    "supertest-fetch": "^2.0.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.7.3",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-pwa": "^1.0.1",
    "vitest": "^3.2.4"
  }
}
